// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Brand {
  id String @id @db.Uuid
  name String
  created_at Float
  updated_at Float
  last_modified Float
  server_created_at Float
  deleted Boolean @default(false)
}


model Manufacturers {
  id String @id @db.Uuid
  name String
  created_at Float
  updated_at Float
  last_modified Float
  isTemporary Boolean @default(false)
  server_created_at Float
  deleted Boolean @default(false)
}


model Internal_Business_Categories {
  id String @id @db.Uuid
  name String
  description String
  created_at Float
  updated_at Float
  deleted Boolean @default(false)
}

