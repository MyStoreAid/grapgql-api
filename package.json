{
  "name": "graphql-api",
  "version": "0.0.1",
  "description": "MyStoreAid graphql-api",
  "main": "server.js",
  "scripts": {
    "dev": "npx prisma generate && nodemon src/server.ts",
    "start": "npx prisma generate && node src/server.js",
    "prod": "npx prisma generate && npm run build && node src/server.js",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "build": "tsc -p .",
    "set-test-env": "dotenv -e .env.test",
    "migrate-dev": "dotenv -e .env.test -- npx prisma migrate dev",
    "set_test_env": "dotenv -e .env.test -- docker-compose -f docker-compose-test.yml up -d && npm run migrate-dev",
    "test": "npm run set_test_env && dotenv -e .env.test -- jest -i",
    "wunderflats": "nodemon src/wunderflats.ts"
  },
  "author": "salifumutaru@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@mystoreaid/backend-helpers": "^1.0.5",
    "@mystoreaid/prisma-models": "^1.5.6",
    "@prisma/client": "^2.27.0",
    "@types/uuid": "^8.3.1",
    "apollo-server": "^3.0.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "geoip-lite": "^1.4.2",
    "getenv": "^1.0.0",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.13.6",
    "graphql-tools": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "pino": "^6.13.0",
    "prisma": "^2.27.0",
    "save-dev": "0.0.1-security",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.4.2"
  }
}
